<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UIControllerMainMenu">

  <children>
      <Button id="btNewGame" fx:id="btNewg" mnemonicParsing="false" onMouseClicked="#btNewGameOnClickHandler" text="New Game">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button id="btSettings" fx:id="btSett" alignment="CENTER" onMouseClicked="#btSettingsOnClickHandler" text="Settings">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button id="btProfile" fx:id="btProf" mnemonicParsing="false" onMouseClicked="#btProfileOnClickHandler" text="Profile">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
      <Button id="btExit" fx:id="btExit" mnemonicParsing="false" onMouseClicked="#btExitOnClickHandler" text="Exit">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Button>
  </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
